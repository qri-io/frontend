    "deep-equal": "2.0.5",
    "dotenv": "8.2.0",
    "history": "4.10.1",
    "install": "0.13.0",
    "json-schema": "0.3.0",
    "jwt-decode": "3.1.2",
    "markdown-it": "12.2.0",
    "monaco-editor-webpack-plugin": "4.0.0",
    "node-fetch": "2.6.1",
    "node-sass": "4.14.1",
    "numeral": "2.0.6",
    "playwright": "1.14.1",
    "postcss": "7",
    "puppeteer": "5.5.0",
    "query-string": "7.0.0",
